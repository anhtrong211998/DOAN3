
@{
    ViewBag.Title = "DanhSachNhanVien";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
<script src="~/Scripts/plugins/jquery.twbsPagination.min.js"></script>
<script src="~/Scripts/AjaxJson/QuanLyNhanVienajax.js"></script>
<section id="main-content">
    <section class="wrapper">
        <div class="dulieu">
            <div class="dl_motkhoi">
                <div class="dl_trai">
                    <h3 class="solieu" id="slsp"></h3>
                    <span>Nhân viên</span>
                </div>
                <div class="dl_phai">
                    <i class="fa fa-eye"> </i>
                </div>
                <div class="clearfix"> </div>
            </div>
        </div>
        <div class="card_layer">
            <h5>Danh sách nhân viên</h5>
            <div class="row">
                <div class="col-md-12">
                    <button id="btnAddNew" class="btn btn-success pull-right">Thêm</button>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <td>Mã nhân viên</td>
                                <td>Tên nhân viên</td>
                                <td>Hình ảnh</td>
                                <td>Chức vụ</td>
                                <td>Quê quán</td>
                                <td>Email</td>
                                <td>Số điện thoại</td>
                                <td>Tài khoản</td>
                                <td>Chức năng</td>
                            </tr>
                        </thead>
                        <tbody id="tblData"></tbody>
                    </table>
                    <div id="pagination" class="pagination">
                    </div>
                </div>
            </div>
        </div>
    </section>
</section>
@Html.Partial("~/Areas/Admin/Views/QuanLyNhanVien/NhanVienModalPartialView.cshtml");